#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

echo "-----> unzipping ratpack"
mkdir zip
unzip ratpack.zip -d zip
echo "-----> current dir"
mkdir jar
unzip -o zip/example-ratpack-gradle-groovy-app-fat.jar -d jar
cp -R jar/* $1
echo "-----> build dir"
ls -alh $1
echo "-----> Done with compile"
